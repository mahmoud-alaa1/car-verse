{
  "canisters": {
    "backend": {
      "candid": "src/backend/backend.did",
      "package": "backend",
      "type": "rust"
    },
    "frontend": {
      "dependencies": [
        "backend"
      ],
      "source": [
        "src/frontend/dist"
      ],
      "type": "assets",
      "workspace": "frontend",
      "build": "pnpm install && pnpm run build"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "output_env_file": ".env",
  "version": 1
}